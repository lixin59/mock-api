{
  "name": "mock_api",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src/**/*.{jsx,js,ts,tsx,vue} --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,css,less,scss,vue,html,ts,tsx,md}\"",
    "prepare": "husky install",
    "add:husky-pre-commit": "husky add .husky/pre-commit 'npm run lint-staged'",
    "add:husky-commit-msg": "husky add .husky/pre-commit 'yarn commitlint --config commitlint.config.js --edit $1'",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "npm run lint",
      "git add src/"
    ]
  },
  "dependencies": {
    "axios": "^0.26.1",
    "nprogress": "^0.2.0",
    "primeicons": "^5.0.0",
    "primevue": "^3.15.0",
    "type-gverify": "^1.0.3",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14",
    "vuesax": "^4.0.1-alpha.25"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.26",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "prettier": "^2.6.2",
    "sass": "^1.50.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^0.34.7"
  }
}
